From 4a7354ed576b1b51ccca6e4f0674df198454fdea Mon Sep 17 00:00:00 2001
From: Antonio Rojas <arojas@archlinux.org>
Date: Fri, 22 Dec 2023 20:11:43 +0100
Subject: [PATCH] Fix build with Qt 6.7

Add another target alias needed to work around QTBUG-114706

BUG: 478879
---
 qt6/CMakeLists.txt | 3 +++
 1 file changed, 3 insertions(+)

diff --git a/qt6/CMakeLists.txt b/qt6/CMakeLists.txt
index a64dfa50..726fe10e 100644
--- a/qt6/CMakeLists.txt
+++ b/qt6/CMakeLists.txt
@@ -11,6 +11,9 @@ add_library(Qt::Core ALIAS Qt6::Core)
 add_library(Qt::Network ALIAS Qt6::Network)
 add_library(Qt::OpenGL ALIAS Qt6::OpenGL)
 add_library(Qt::Gui ALIAS Qt6::Gui)
+if(Qt6_VERSION_MINOR GREATER 6)
+    add_library(Qt::PlatformModuleInternal ALIAS Qt6::PlatformModuleInternal)
+endif()
 
 find_package(KF6 ${KF6_MIN_VERSION} REQUIRED COMPONENTS
                 Config ConfigWidgets I18n IconThemes KIO Notifications
